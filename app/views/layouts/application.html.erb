<!DOCTYPE html>
<html>
<head>
  <title>Movie Cart</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag     'application', 'http://fonts.googleapis.com/css?family=Bevan' %>
  <%= stylesheet_link_tag     'application', 'http://fonts.googleapis.com/css?family=Montserrat' %>
  <%= stylesheet_link_tag     'application', 'http://fonts.googleapis.com/css?family=Roboto:300' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class='<%= controller.controller_name %>'>
  <div id=wrapper>
    <%= yield %>
  </div>
</body>
</html>
